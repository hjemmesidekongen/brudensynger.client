{
  "name": "brudensynger.client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "NODE_ENV=production npm run build && npm run export",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "test:coverage": "npm run test -- --coverage --colors"
  },
  "dependencies": {
    "formik": "^2.0.4",
    "next": "9.1.3",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-textarea-autosize": "^7.1.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "history": "^4.10.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-prop-type-error": "^1.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "pages/**/*.{js,jsx}": [
      "eslint",
      "pretty-quick — staged",
      "git add"
    ],
    "components/**/*.{js,jsx}": [
      "eslint",
      "pretty-quick — staged",
      "git add"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-prop-type-error"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/components/**/*.js",
      "<rootDir>/pages/**/*.js"
    ]
  }
}
